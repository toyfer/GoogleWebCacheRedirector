{
    "manifest_version": 2,
    "name": "Google Web Cache",
    "version": "1.0",
    "permissions": [
        "storage",
        "https://www.google.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*/*"
            ],
            "run_at": "document_start" ,
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}